{
  "expo": {
    "name": "Food App",
    "slug": "food_app_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Landing.png",
    "splash": {
      "image": "./assets/Landing.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF4B3A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.seun.foodApp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
